{
  "buildCommand": "npm --prefix react-frontend install && npm --prefix react-frontend run build",
  "outputDirectory": "react-frontend/dist",
  "installCommand": "npm --version",
  "devCommand": "npm --prefix react-frontend run dev",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
